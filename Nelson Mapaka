import pandas as pd
import matplotlib.pyplot as plt
data = pd.read_csv(
	'x_y_coordinates.txt', 
	delimiter=',', 
	header=None, 
	names=['x', 'y']
)
print("Column names:", data.columns)
plt.scatter(data['x'], data['y'])
plt.title('Scatter Plot of Coordinates')
plt.xlabel('x_axis')
plt.ylabel('y_axis')
plt.grid(False)
plt.show()